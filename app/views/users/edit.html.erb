<h1>Editar</h1>

<%= form_with model: @user do |form| %>
    <div>
      <%= form.label :name %><br>
      <%= form.text_field :name %>
    </div>
  
    <div>
      <%= form.label :lastname %><br>
      <%= form.text_field :lastname %>
    </div>

    <div>
      <%= form.label :email %><br>
      <%= form.text_field :email %>
    </div>
  
    <div>
      <%= form.submit t("user.save") %>
      <p><%= link_to "Users", users_path%></p>
    </div>
  <% end %>